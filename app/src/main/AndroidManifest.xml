<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.romijulianto.educationxyz">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="Build With Romi Julianto"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Educationxyz"
        tools:targetApi="31">
        <activity
            android:name=".about.About"
            android:exported="false" />
        <activity
            android:name=".course.CourseDetail"
            android:exported="false" />
        <activity
            android:name=".onboarding.OnBoarding"
            android:exported="false" />
        <activity
            android:name=".onboarding.StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="Build With Romi Julianto" />
    </application>

</manifest>